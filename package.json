{
  "name": "motion-svg",
  "version": "0.1.0",
  "description": "Declarative SVG animation engine â€” parse, wrap, animate & export SVG paths with keyframes, easing curves, and interaction triggers",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    },
    "./vanilla": {
      "types": "./dist/vanilla/index.d.ts",
      "import": "./dist/vanilla/index.js",
      "require": "./dist/vanilla/index.cjs"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.js",
      "require": "./dist/plugins/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "svg",
    "animation",
    "keyframes",
    "easing",
    "motion",
    "interactive",
    "declarative"
  ],
  "license": "MIT",
  "author": "vectormotion",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vectormotion/motion-svg.git"
  },
  "bugs": {
    "url": "https://github.com/vectormotion/motion-svg/issues"
  },
  "homepage": "https://github.com/vectormotion/motion-svg#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": { "optional": true },
    "react-dom": { "optional": true }
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "vitest": "^1.6.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "@types/react": "^18.3.0"
  }
}
